{
    "name": "illuminatech/sync-many-attribute",
    "description": "Allows control over Eloquent many-to-many relation via array attribute",
    "keywords": ["laravel", "eloquent", "many-to-many", "attribute", "sync"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/sync-many-attribute/issues",
        "wiki": "https://github.com/illuminatech/sync-many-attribute/wiki",
        "source": "https://github.com/illuminatech/sync-many-attribute"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^6.0 || ^7.0"
    },
    "require-dev": {
        "illuminate/events": "^6.0 || ^7.0",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\SyncManyAttribute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\SyncManyAttribute\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}